<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hlsfile.mapper.LocalDepartmentMapper">

    <!-- 开启二级缓存 -->
    <!-- <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/> -->

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.hlsfile.bean.LocalDepartment">
        <id column="local_id" property="localId" />
        <result column="local_name" property="localName" />
        <result column="local_level" property="localLevel" />
        <result column="QR_path" property="qrPath" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        local_id, local_name, local_level, QR_path
    </sql>

</mapper>
